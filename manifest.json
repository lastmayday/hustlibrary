{
   "browser_action": {
      "default_icon": "icon.png"
   },
   "content_scripts": [ {
      "js": [ "jquery.js", "inject.js" ],
      "matches": [ "http://book.douban.com/subject/*" ],
      "run_at": "document_end"
   } ],
   "description": "豆瓣读书页面直接查询华科图书馆有没有",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png"
   },
   "manifest_version": 2,
   "name": "华科图书馆豆瓣读书查询",
   "permissions": [ "tabs", "http://ftp.lib.hust.edu.cn/search*" ],
   "version": "1.2"
}
